{
  "name": "mern",
  "version": "1.0.0",
  "description": "Scottsdale Event Decor",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "mocha"
  },
  "author": "Trevor Johnson, Tia Sirrine, Josh Hinton, Wing Wong",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "availity-reactstrap-validation": "^2.0.2",
    "axios": "^0.16.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "csv": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "json2csv": "^4.1.6",
    "moment": "^2.22.2",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "nodemailer": "^4.6.7",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react-images": "0.5.17",
    "react-photo-gallery": "^6.0.28",
    "router": "^1.3.2",
    "sequelize": "^4.38.0"
  }
}
